#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/@crawlee+cli@3.14.1/node_modules/@crawlee/cli/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/@crawlee+cli@3.14.1/node_modules/@crawlee/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/@crawlee+cli@3.14.1/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/@crawlee+cli@3.14.1/node_modules/@crawlee/cli/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/@crawlee+cli@3.14.1/node_modules/@crawlee/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/@crawlee+cli@3.14.1/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@crawlee/cli/index.js" "$@"
else
  exec node  "$basedir/../@crawlee/cli/index.js" "$@"
fi

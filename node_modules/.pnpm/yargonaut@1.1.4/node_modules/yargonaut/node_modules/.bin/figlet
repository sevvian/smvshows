#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/figlet@1.8.2/node_modules/figlet/bin/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/figlet@1.8.2/node_modules/figlet/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/figlet@1.8.2/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/figlet@1.8.2/node_modules/figlet/bin/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/figlet@1.8.2/node_modules/figlet/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/figlet@1.8.2/node_modules:/home/mohan/dyad-apps/smvshows/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../figlet@1.8.2/node_modules/figlet/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../figlet@1.8.2/node_modules/figlet/bin/index.js" "$@"
fi
